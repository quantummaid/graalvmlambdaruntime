<ruleset comparisonMethod="maven"
         xmlns="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0 http://mojo.codehaus.org/versions-maven-plugin/xsd/rule-2.0.0.xsd">
    <ignoreVersions>
        <ignoreVersion type="regex">
            .*[-_\.](alpha|Alpha|ALPHA|beta|Beta|BETA|rc|RC|M|EA)[-_\.]?[0-9]?.*
        </ignoreVersion>
    </ignoreVersions>
    <rules>
        <rule groupId="io.github.microutils" comparisonMethod="kotlin-logging">
            <ignoreVersions>
                <ignoreVersion>2.0.10</ignoreVersion>
                <ignoreVersion>2.0.9</ignoreVersion>
                <ignoreVersion>2.0.8</ignoreVersion>
                <ignoreVersion>2.0.7</ignoreVersion>
                <ignoreVersion>2.0.6</ignoreVersion>
                <ignoreVersion>2.0.5</ignoreVersion>
                <ignoreVersion>2.0.4</ignoreVersion>
                <ignoreVersion>2.0.3</ignoreVersion>
                <ignoreVersion>2.0.2</ignoreVersion>
                <ignoreVersion>2.0.1</ignoreVersion>
                <ignoreVersion>2.0.0</ignoreVersion>
            </ignoreVersions>
        </rule>
    </rules>
</ruleset>